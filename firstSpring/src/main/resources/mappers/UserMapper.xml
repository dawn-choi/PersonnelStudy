<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="co.study.firstSpring.mapper.UserMapper">

  <select id="findAll" resultType="User">
    select id, name
    from users
    order by id
  </select>

</mapper>
